{
  "tags": [
    {
      "name": "{{tableName}}",
      "description": "{{desc}}"
    }
  ],
  "definitions": {
    "{{tableName}}": {
      "type": "object",
      "properties": {
       {% for field in fields %}
        "{{ field.name }}": {
          "type": "{{ field.type }}",
          "description": "{{ field.desc }}"
        },
        {% endfor %}
        "id": {
            "type": "string",
            "description": "{{tableName}} 的 ID"
        }
      }
    }
  },
  "paths": {
    "/row/{{tableName}}/{id}": {
      "get": {
        "summary": "获取 {{tableName}} 详情",
        "description": "根据 ID 获取 {{tableName}} 详情",
        "tags": [
          "{{tableName}}"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "description": "{{tableName}} 的 ID",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "返回 {{tableName}} 数据",
            "schema": {
              "$ref": "#/definitions/{{tableName}}"
            }
          },
          "404": {
            "description": "object not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "put": {
        "summary": "更新 {{tableName}}",
        "description": "通过ID更新 {{tableName}}",
        "tags": [
          "{{tableName}}"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "description": "更新 {{tableName}} 的 ID",
            "required": true
          },
          {
            "in": "body",
            "name": "body",
            "description": "数据更新的内容.",
            "required": true,
            "schema": {
              "$ref": "#/definitions/{{tableName}}"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "更新成功",
            "schema": {
              "type": "object",
              "properties": {
                "updatedAt": {
                  "description": "更新时间",
                  "type": "string",
                  "format": "date-time"
                },
                "objectId": {
                  "description": "objectId",
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "object not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "delete": {
        "summary": "删除 {{tableName}}",
        "description": "通过 ID 删除 {{tableName}} 记录",
        "tags": [
          "{{tableName}}"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "description": "删除 {{tableName}} 的 ID",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Returns a confirmation message"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "404": {
            "description": "object not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/table/{{tableName}}": {
      "get": {
        "summary": "查询 {{tableName}}",
        "description": "where支持：[$lt, $lte, $gt, $gte, $ne, $in, $nin, $exists, $select, $dontSelect, $all, $regex, $text]",
        "tags": [
          "{{tableName}}"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "order",
            "description": "排序方式, 负为降序, eg: score,-name",
            "required": false,
            "type": "string",
            "default": "createdAt"
          },
          {
            "in": "query",
            "name": "limit",
            "description": "limit, 显示多少条记录，显示 100 条记录 eg:100",
            "required": false,
            "default": 10,
            "type": "integer"
          },
          {
            "in": "query",
            "name": "skip",
            "description": "skip",
            "required": false,
            "default": 0,
            "type": "integer"
          },
          {
            "in": "query",
            "name": "keys",
            "description": "需要显示的字段,加 count(*) 则结果里面会显示总记录数, eg: username,email",
            "required": false,
            "type": "string"
          },
          {
            "in": "query",
            "name": "include",
            "description": "include",
            "required": false,
            "type": "string"
          },
          {
            "in": "query",
            "name": "where",
            "description": "过滤条件，eg: {\"objectId\":{\"$in\":[\"BGS3BFgcZN\"]}}",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "返回 {{tableName}} 记录数",
            "schema": {
              "type": "object",
              "properties": {
                "results": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/{{tableName}}"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "post": {
        "summary": "新增 {{tableName}}",
        "description": "新增一条 {{tableName}} 记录",
        "tags": [
          "{{tableName}}"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "新增的字段",
            "required": true,
            "schema": {
              "$ref": "#/definitions/{{tableName}}"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Returns success",
            "schema": {
              "type": "object",
              "properties": {
                "createdAt": {
                  "description": "创建时间",
                  "format": "date-time",
                  "type": "string"
                },
                "objectId": {
                  "description": "objectId",
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/schema/{{tableName}}": {
      "get": {
        "summary": "获取 {{tableName}} 表的结构",
        "description": "获取 {{tableName}} 表的结构",
        "tags": [
          "{{tableName}}"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "获取成功"
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    }
  }
}
