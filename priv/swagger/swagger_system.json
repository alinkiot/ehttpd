{
  "tags": [
    {
      "name": "system",
      "description": "系统操作接口"
    }
  ],
  "definitions": {
    "Swagger": {
      "type": "object",
      "properties": {
        "definitions": {
          "description": "Models",
          "type": "object"
        },
        "paths": {
          "description": "路径",
          "type": "object"
        }
      }
    }
  },
  "paths": {
    "/generate/api": {
      "post": {
        "permission": "system:api:gen",
        "summary": "根据 swagger 产生 API 代码",
        "description": "根据 swagger 产生 API 代码",
        "parameters": [
          {
            "in": "query",
            "name": "mod",
            "type": "string",
            "description": "模块名",
            "required": true
          },
          {
            "in": "body",
            "name": "swagger",
            "description": "swagger 描述",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Swagger"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "tags": [
          "system"
        ]
      }
    },
    "/generate/page": {
      "post": {
        "permission": "system:page:gen",
        "summary": "根据表产生 vue 代码",
        "description": "根据表产生 vue 代码",
        "parameters": [
          {
            "in": "query",
            "name": "table",
            "type": "string",
            "description": "表名",
            "required": true
          },
          {
            "in": "body",
            "name": "swagger",
            "description": "swagger 描述",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Swagger"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "tags": [
          "system"
        ]
      }
    },
    "/upload": {
      "post": {
        "permission": "system:file:upload",
        "summary": "服务器文件上传",
        "description": "文件上传到服务器",
        "consumes": [
          "multipart/form-data"
        ],
        "parameters": [
          {
            "name": "file",
            "in": "formData",
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "tags": [
          "system"
        ]
      }
    },
    "/monitor/server": {
      "get": {
        "permission": "system:monitor:query",
        "summary": "获取系统详情",
        "description": "获取系统详情",
        "tags": [
          "system"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "返回服务器数据"
          },
          "404": {
            "description": "object not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/monitor/cache": {
      "get": {
        "permission": "system:cache:query",
        "summary": "获取缓存详情",
        "description": "获取缓存详情",
        "tags": [
          "system"
        ],
        "parameters": [
        ],
        "responses": {
          "200": {
            "description": "返回服务器数据"
          },
          "404": {
            "description": "object not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    }
  }
